{
  "KeepAlive": {
    "Sites": "https://pegasus.local;https://pegasusapi.local",
    "TaskDelay": "120000",
    "NotificationDelay": "86400000"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs/PegasusService.log"
        }
      }
    ]
  }
}
